Match Host *.tu-berlin.de
    KbdInteractiveAuthentication no
    GSSAPIAuthentication yes
    GSSAPIDelegateCredentials yes
    User aljoschafrey

Match Host *.tu-berlin.de,!sshgate.tu-berlin.de,!gitlab.tu-berlin.de
    ProxyJump sshgate.tu-berlin.de

Host * !*.* !github.com
    SendEnv LC_REMOTE_USER
    # add 'environment="LC_REMOTE_USER=aljoschua" ssh-rsa...' to authorized_keys if this doesnt work
    # Reread inputrc and others ?

    ControlMaster auto
    ControlPath ~/.ssh/socket:%h:%p
    # Does not work with ssh-copy-rc..
    ControlPersist 600
    LocalCommand ~/.ssh/ssh-copy-rc %r %h %p

Host *
    CanonicalizeHostname yes
    CanonicalDomains eecsit.tu-berlin.de
    IdentityFile ~/.ssh/430-G5
